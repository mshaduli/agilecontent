    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="tne_operator_listing_index" pattern="/operators">
        <default key="_controller">TNEOperatorBundle:Default:index</default>
