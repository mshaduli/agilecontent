<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="TNE\OperatorBundle\Entity\DestinationRepository" name="TNE\OperatorBundle\Entity\Destination">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" column="name" length="255"/>
    <field name="latitude" type="decimal" column="latitude" scale="7" nullable="true"/>
    <field name="longitude" type="decimal" column="longitude" scale="7" nullable="true"/>
    <field name="state" type="string" column="state" length="255"/>
  </entity>
</doctrine-mapping>
